{
  "name": "OpenFinAL",
  "displayName": "OpenFinAL",
  "productName": "OpenFinAL",
  "version": "0.1.0",
  "description": "An AI-enabled software to learn financial investing",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "debug": "ELECTRON_ENABLE_LOGGING=1 electron-forge start",
    "debug-main": "electron-forge start -- --inspect-electron=9229",
    "debug-renderer": "electron-forge start -- --remote-debugging-port=9222",
    "clean": "rm -rf .webpack out node_modules package-lock.json && npm install && npm run postinstall",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "postinstall": "electron-rebuild --version 35.1.5",
    "build": "electron-builder --win --x64",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Jeff Wall",
    "email": "jeffrey.d.wall@gmail.com"
  },
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-react": "^7.26.3",
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron-forge/plugin-webpack": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "babel-loader": "^10.0.0",
    "browserify-fs": "^1.0.0",
    "copy-dependencies": "file:./forge-plugins",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.11.0",
    "electron": "35.1.5",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "node-loader": "^2.1.0",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "assert": "^2.1.0",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.9.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.1",
    "electron-squirrel-startup": "^1.0.1",
    "events": "^3.3.0",
    "express": "^5.1.0",
    "file-saver": "^2.0.5",
    "howler": "^2.2.4",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "keytar": "^7.9.0",
    "node-html-parser": "^7.0.1",
    "os-browserify": "^0.3.0",
    "puppeteer": "^24.6.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.0",
    "react-tiny-popover": "^8.1.6",
    "recharts": "^2.15.2",
    "sqlite3": "^5.1.7",
    "stream-http": "^3.2.0",
    "tls": "^0.0.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "util": "^0.12.5",
    "uuid": "^11.1.0",
    "xml2js": "^0.6.2",
    "yahoo-finance2": "^2.13.3"
  },
  "build": {
    "appId": "com.openfinal.app",
    "productName": "OpenFinAL",
    "win": {
      "target": [
        "portable"
      ]
    },
    "asar": true,
    "asarUnpack": [
      "**/*.node"
    ],
    "extraResources": [
      {
        "from": "node_modules/better-sqlite3/build/Release/better_sqlite3.node",
        "to": "better_sqlite3.node"
      },
      {
        "from": "node_modules/better-sqlite3",
        "to": "node_modules/better-sqlite3",
        "filter": [
          "**/*.js",
          "**/package.json"
        ]
      }
    ],
    "files": [
      "src/**/*",
      "node_modules/**/*",
      "!node_modules/better-sqlite3/build/Release/obj/**/*",
      "!node_modules/better-sqlite3/build/Release/*.pdb"
    ]
  }
}
